<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="web.dao.face.BoardFileDao">

	<sql id="selectBoardFile">
		SELECT
			file_no, board_no, origin_name, stored_name
		FROM boardfile
	</sql>
  
  
	<delete id="deleteByBoardNo" parameterType="BoardFile">
		DELETE boardfile
		<where>
			board_no = #{boardNo}
		</where>
	
	</delete>
	
	
	
</mapper>